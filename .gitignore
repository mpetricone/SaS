# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal


#Ignore storage
/storage

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

/o-doc

/public/packs
/public/packs-test
/public/assets
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity

.byebug_history

/app/assets/builds/*
!/app/assets/builds/.keep

