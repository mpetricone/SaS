<%= render partial: 'shared/error_form_uc', locals: { result: @distributer_email, object_name: DistributerEmail.model_name.human, object_action: form_action } %>
<%= form_for [@distributer, @distributer_email] do |f| %>
    <div class='row'>
        <%= render_field_group f, DistributerEmail.model_name.human,
            [{label: :email, name: :email, type: :text_field},
             {label: :description, name: :description, type: :text_field}] %>
     </div>
     <%= render_actions f, @distributer %>
 <% end %>
