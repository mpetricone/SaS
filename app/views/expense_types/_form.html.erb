<%= render partial: 'shared/error_form_uc', locals: { result: @expense_type, object_name: Expense.model_name.human, object_action: form_action } %>
<%= form_for(@expense_type) do |f| %>
  <div class='row'>
    <%= render_field_group f, t(:label_primary_info),
        [{label: :name, name: :name, type: :text_field},
         {label: :description, name: :description, type: :text_field}] %>
     </div>
     <%= render_actions f, @expense_type %>
<% end %>
