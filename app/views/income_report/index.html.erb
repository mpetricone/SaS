<%= render partial: 'shared/form_page_title', locals: { title: t(:title_income_report) } %>
<div class='row'>
  <div class='col-12' id='search'>
    <div class='card'>
      <%= content_tag :div, t(:sub_title_by_payment), class: 'card-header' %>
      <div class='card-body'>
        <%= form_tag income_report_report3_path, method: :get, remote: true, class: 'form-horizontal' do %>
          <div class='form-group'>
            <%= content_tag :label, t(:label_start_date), for: :date_start, class: 'col-xs-5' %>
            <%= date_field :search2, :date_start, value: Date.today, class: 'col-xs-7' %><br/>
          </div>
          <div class='form-group'>
            <%= content_tag :label, t(:label_end_date), for: :date_end, class: 'col-xs-5' %>
            <%= date_field :search2, :date_end, value: Date.today, class: 'col-xs-7' %><br/>
          </div>
          <div class='form-group'>
            <%= submit_tag "Search", class: 'btn btn-primary' %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
<div class='row' id='result'>
</div>
