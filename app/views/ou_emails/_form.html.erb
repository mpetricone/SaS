<%= render partial: 'shared/error_form_uc', locals: { result: @ouEmail, object_name: 'OU E-mail', object_action: form_action } %>
<%= form_for [@ou, @ouEmail] do |f| %>
    <div class='row'>
        <%= render_field_group f, OuEmail.model_name.human, 
            [{label: :address, name: :address, type: :text_field},
             { label: :description, name: :description, type: :text_field}] %>
    </div>
    <%= render_actions f, @ou %>
<% end %>
