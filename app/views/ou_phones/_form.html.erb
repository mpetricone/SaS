<%= render partial: 'shared/error_form_uc' , locals: { result: @ouPhone, object_name: 'OU Phone',object_action: form_action } %>
<%= form_for [ @ou, @ouPhone ] do |phone| %>
    <div class='row'>
        <%= render_field_group phone, OuPhone.model_name.human, 
            [{label: :number, name: :number, type: :text_field},
             {label: :description, name: :description, type: :text_field}]%>
     </div>
     <%= render_actions phone, @ou %>
 <%end%>
