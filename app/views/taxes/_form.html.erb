<%= render partial: 'shared/error_form_uc', locals: {result: @tax, object_name: Tax.model_name.human, object_action: form_action } %>
<%= form_for @tax do |f| %>
    <div class='row'>
        <%= render_field_group f, t(:label_primary_info),
            [{ label: :name, name: :name, type: :text_field },
             { label: :rate, name: :rate, type: :text_field },
             { label: :region, name: :region, type: :text_field }] %>

         <%= render_field_group f, t(:label_dates),
             [{ label: :date_enabled, name: :date_enabled, type: :date_field},
              { label: :date_retired, name: :date_retired, type: :date_field}] %>
      </div>
      <%= render_actions f, taxes_path %>
  <%end%>
