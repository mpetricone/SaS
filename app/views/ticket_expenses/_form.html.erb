<%= render partial: 'shared/error_form_uc', locals: {
	result: @ticket_expense, object_name: TicketExpense.model_name.human, object_action: form_action } %>
<%= form_for [@ticket, @ticket_expense] do |f| %>
	<div class='form-body'>
		<%= render_field_group f, TicketExpense.human_attribute_name(:date_incurred),
			[{label: nil, name: :date_incurred, type: :date_field}] %>
		<%= render_field_group f, t(:label_primary_info),
			[{ label: TicketExpense.human_attribute_name(:expense_type), name: :expense_type_id, type: :collection_select,
			ext_params: { collection: ExpenseType.all, value_method: :id, text_method: :name} },
		{ label: TicketExpense.human_attribute_name(:note), name: :note, type: :text_area,options: { size: "40x4" } }] %>
	<%= render_field_group f, TicketExpense.human_attribute_name(:cost),
		[{label: nil, name: :cost, type: :text_field }] %>
	</div>
	<%= render_actions f, @ticket %>
<% end %>
