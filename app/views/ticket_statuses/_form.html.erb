<%= render partial: 'shared/error_form_uc', locals: { result: @ticket_status, object_name: TicketStatus.model_name.human, object_action: form_action } %>
<%= form_for @ticket_status do |f| %>
    <div class='row'>
        <%= render_field_group f, TicketStatus.human_attribute_name(:name),
            [{ label: :name, name: :name, type: :text_field } ] %>
    </div>
    <%= render_actions f, ticket_statuses_path %>
<% end %>
